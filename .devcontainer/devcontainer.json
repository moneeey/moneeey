{
	"name": "Moneeey Development",
	"dockerComposeFile": [
		"../docker-compose.yaml",
		"docker-compose.workspace.yaml"
	],
	"service": "workspace",
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint",
				"esbenp.prettier-vscode",
				"ms-azuretools.vscode-docker",
				"christian-kohler.npm-intellisense",
				"christian-kohler.path-intellisense"
			],
			"settings": {
				"remote.containers.remoteServerDataFolder": "/home/node/.vscode-server"
			}
		}
	},
	"forwardPorts": [4280],
	"remoteUser": "node",
	"containerUser": "node",
	"features": {
		"ghcr.io/devcontainers/features/common-utils:2": {}
	},
	"containerEnv": {
		"HOME": "/home/node",
		"UID": "${localEnv:UID:-1000}",
		"GID": "${localEnv:GID:-1000}"
	},
	"postCreateCommand": {
		"fixVolumePerms": "sudo chown -R $(whoami): /workspace",
		"gitSafeWorkspace": "git config --global --add safe.directory /workspace"
	},
	"overrideCommand": false,
	"updateRemoteUserUID": true,
	"mounts": [
		"source=${localEnv:HOME}${localEnv:USERPROFILE}/.vscode-server,target=/home/node/.vscode-server,type=bind,consistency=cached"
	]
}
